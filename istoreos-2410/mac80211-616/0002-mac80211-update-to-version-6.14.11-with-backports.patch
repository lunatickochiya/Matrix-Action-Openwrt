++		evm[RF_PATH_D] = le32_get_bits(rpt->w5, RTW_JGRPHY_W5_RXEVM_4);
++
++		if (pkt_stat->rate <= DESC_RATE54M)
++			evm[RF_PATH_A] = le32_get_bits(rpt->w6,
++						       RTW_JGRPHY_W6_SIGEVM);
++
++		for (i = RF_PATH_A; i < RTW_RF_PATH_MAX; i++) {
