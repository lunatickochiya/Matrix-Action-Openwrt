-+		if (write_1ss_mcs9)
-+			mask = MASKLWORD;
-+		else
-+			mask = MASKDWORD;
-+
-+		if (shift == 0x3 || write_1ss_mcs9) {
-+			rate_idx = rate & 0xfc;
