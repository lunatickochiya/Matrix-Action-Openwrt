-+static void rtw8812a_do_iqk(struct rtw_dev *rtwdev)
-+{
-+	static const u32 backup_macbb_reg[MACBB_REG_NUM_8812A] = {
-+		0x520, 0x550, 0x808, 0xa04, 0x90c, 0xc00, 0xe00, 0x838, 0x82c
-+	};
