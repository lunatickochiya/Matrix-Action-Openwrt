++++ b/drivers/net/wireless/ath/ath11k/hw.h
+@@ -233,6 +233,7 @@ struct ath11k_hw_params {
+ 	bool support_fw_mac_sequence;
+ 	bool support_dual_stations;
+ 	bool pdev_suspend;
++	bool reo_status_poll;
+ };
