-+	val16 &= ~BIT_RFMOD;
-+	if (bw == RTW_CHANNEL_WIDTH_80)
-+		val16 |= BIT_RFMOD_80M;
-+	else if (bw == RTW_CHANNEL_WIDTH_40)
-+		val16 |= BIT_RFMOD_40M;
-+
-+	rtw_write16(rtwdev, REG_WMAC_TRXPTCL_CTL, val16);
