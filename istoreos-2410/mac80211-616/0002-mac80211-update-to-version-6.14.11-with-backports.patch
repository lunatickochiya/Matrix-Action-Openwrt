-+	rtw_pwr_seq_parser(rtwdev, enter_lps_flow);
-+
-+	if (rtw_read8(rtwdev, REG_MCUFW_CTRL) & BIT_RAM_DL_SEL)
-+		rtw88xxa_reset_8051(rtwdev);
