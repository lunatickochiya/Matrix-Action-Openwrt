-+			if (efuse->ext_pa_5g) {
-+				if (efuse->ext_lna_2g && efuse->ext_pa_2g)
-+					efuse->rfe_option = 3;
-+				else
-+					efuse->rfe_option = 0;
-+			} else {
-+				efuse->rfe_option = 2;
