-+	for (j = 0; j < rtw_rate_size[rs]; j++) {
-+		rate = rtw_rate_section[rs][j];
-+
-+		pwr_index = hal->tx_pwr_tbl[path][rate];
-+
-+		shift = rate & 0x3;
